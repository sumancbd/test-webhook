{
  "name": "@monorepo/api",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "pnpm prisma generate && nest build",
    "console": "node dist/src/console.js",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "copy-env": "shx cp .env.example .env",
    "createAdminUser": "ts-node src/prisma/createAdminUser.ts",
    "dev": "pnpm run start:debug",
    "dev:fixtures": "pnpm run console:dev fixtures",
    "dev:seeders": "pnpm run console:dev seeders",
    "dev:mongo": "ts-node ./scripts/devMongo.ts",
    "dev:redis": "ts-node ./scripts/devRedis.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen2": "ts-node generator/index.ts",
    "lint": "eslint \"{src,apps,libs,test,json}/**/*.ts\" --fix",
    "prisma": "prisma",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "concurrently --kill-others \"nest start --watch\"  \"prisma generate --watch\"",
    "start:prod": "node dist/src/main",
    "test": "pnpm copy-env; pnpm prisma generate > /dev/null; jest --maxWorkers=50% --workerIdleMemoryLimit='1024MB'",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node",
      "a"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "globalSetup": "<rootDir>/../test/setup.ts",
    "globalTeardown": "<rootDir>/../test/teardown.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.550.0",
    "@golevelup/ts-jest": "^0.4.0",
    "@joi/date": "^2.1.0",
    "@mikro-orm/core": "^5.9.8",
    "@mikro-orm/mongodb": "^5.9.8",
    "@mikro-orm/nestjs": "^5.2.3",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^10.3.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@octokit/webhooks": "^13.2.7",
    "@prisma/client": "^4.16.2",
    "@types/express": "^4.17.21",
    "argon2": "^0.30.3",
    "axios": "^1.6.8",
    "bull": "^4.12.2",
    "dayjs": "^1.11.11",
    "express": "^4.19.2",
    "ffmpeg": "^0.0.4",
    "file-type": "16.5.4",
    "handlebars": "^4.7.8",
    "inquirer": "8.0.0",
    "ioredis": "^5.3.2",
    "jimp": "^0.22.12",
    "joi": "^17.12.3",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.1",
    "nestjs-console": "^9.0.0",
    "nestjs-joi": "^1.10.0",
    "nodemailer": "^6.9.13",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pluralize": "^8.0.0",
    "prisma": "^4.16.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@monorepo/prettier": "workspace:^",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/ffmpeg": "^1.0.7",
    "@types/jest": "29.2.4",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.3.1",
    "mongodb-memory-server": "^9.1.8",
    "msw": "^2.2.13",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "redis-memory-server": "^0.10.0",
    "shx": "^0.3.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5"
  }
}